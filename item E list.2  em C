#include <stdio.h>

int main() {
    // Declaração de variáveis
    float celsius, fahrenheit;

    // Entrada de dados
    printf("Informe a temperatura em Fahrenheit: ");
    scanf("%f", &fahrenheit);

    // Processamento
    celsius = ((fahrenheit - 32) * 5) / 9;

    // Saída de dados
    printf("A temperatura em Celsius é: %.2f graus Celsius\n", celsius);

    return 0;
}
