#include <stdio.h>

int main() {
    // 10 por 7 (Retângulo)
    printf("**********\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("*        *\n");
    printf("**********\n");

    // 7 por 5 (Retângulo menor)
    printf(" ***\n");
    printf("*   *\n");
    printf("*   *\n");
    printf("*   *\n");
    printf("*   *\n");
    printf("*   *\n");
    printf(" ***\n");

    // 9 por 7 (Árvore ou seta)
    printf("    *\n");
    printf("   ***\n");
    printf("  *****\n");
    printf(" *******\n");
    printf("*********\n");
    printf("   *\n");
    printf("   *\n");

    // Losango
    printf("-------------\n");
    printf("      *\n");
    printf("    *  *\n");
    printf("   *    *\n");
    printf("  *      *\n");
    printf("   *    *\n");
    printf("    *  *\n");
    printf("      *\n");

    return 0;
}
